<h1>Listing meetings</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Started at</th>
    <th>Location</th>
    <th>Intro</th>
    <th>Organizer</th>
    
    
    
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @meetings.each do |meeting| %>
  <tr>
    <td><%= meeting.title %></td>
    <td><%= meeting.started_at %></td>
    <td><%= meeting.location %></td>
    <td><%= meeting.title %></td>
    <td><%= meeting.manager %></td>
    <% if current_user %>
    <td><%= link_to 'Enter', meeting %></td>
    

    <td><%= link_to 'Destroy', meeting, confirm: 'Are you sure?', method: :delete %></td>
    <% end %>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meeting', new_meeting_path %>
