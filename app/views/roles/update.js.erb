
<% if @role.assign %>
$('div#assign_<%=dom_id(@role)%> p').html('<%= escape_javascript(@role.assign.username) %>')
$('div#assign_<%=dom_id(@role)%>').attr('class', '<%= assign_status_class(@role) %>  statusRow')
<% else %>
$('div#assign_<%=dom_id(@role)%> p').html("\t")
$('div#assign_<%=dom_id(@role)%>').attr('class', 'nochoice statusRow')
<% end %>

